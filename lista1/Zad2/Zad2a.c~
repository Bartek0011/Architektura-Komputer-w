#include <stdio.h>
#include <unistd.h>
#include <signal.h>

void lapapanie(int n){
	printf("Zlapano sygnal %d \n",n);
}

int main(){
	int i;
	for(i=1;i<=64;++i){
		signal(i,lapanie);
	}
	
	int pid=getpid();
	for(i=1;i<=64;++i){
		printf("Wysylanie sygnalu %d ",i);
		kill(pid,i);
		sleep(1);
	}
}
