#include <stdio.h>
#include <unistd.h>
#include <signal.h>



int main(){

void odb(int q){
	printf("odebrany %d",q);
}

	int k;
	for(k=1;k<=64;k++){
		signal(k, odb);
	
	}
	
	int i;
	for(i=1;i<=64;i++){
		printf("wyslany %d ",i);
	
		kill(getpid(),i);
		printf("\n");
		
	}
return 0;
}
